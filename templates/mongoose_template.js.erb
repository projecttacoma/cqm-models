var Schema = mongoose.Schema;

var <%= datatype %>Schema = new Schema({
  <% for attribute in attributes %><% if attributes.last[:name] == attribute[:name] %>
      <%= attribute[:name] %>: <%= MODELINFO_JS_LOOKUP[attribute[:type]] %>
    <% else %>
      <%= attribute[:name] %>: <%= MODELINFO_JS_LOOKUP[attribute[:type]] %>,
    <% end %>
  <% end %>
});

var <%= datatype %> = mongoose.model('<%= datatype %>', <%= datatype %>Schema);
