{
  "extends": "airbnb-base",
  "rules": {
    "max-len": ["error", {"code": 200, "ignoreComments": true}],
    "no-unused-vars": ["error", { "varsIgnorePattern": "Code|Quantity|Interval|Integer|Array|Float|Time|Number|Date|Mixed" }],
    "camelcase": "off"
  },
  "env": {
    "jasmine": true
  }
}

